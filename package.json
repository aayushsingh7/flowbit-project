{
  "name": "flowbit-ai",
  "private": true,
  "workspaces": [
    "apps/*",
    "services/*",
    "packages/*"
  ],
  "scripts": {
    "build:web": "cd apps/web && npm run build",
    "dev:web": "cd apps/web && npm run dev",
    "start:web": "cd apps/web && npm start",

    "dev:api": "cd apps/api && npm run dev",
    "start:api":"cd apps/api && npm start",

    "dev:vanna": "cd services/vanna && ./venv/bin/python server.py",
    "start:vanna": "cd services/vanna && gunicorn server:app -b 0.0.0.0:5001"
  },
  "devDependencies": {
    "concurrently": "^9.2.1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": ""
}
